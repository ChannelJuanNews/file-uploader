<form id        =  "uploadForm"
     enctype   =  "multipart/form-data"
     action    =  "/api/photo"
     method    =  "post"
>
<input type="file" name="photo" />
<input type="submit" value="Upload Image" name="submit">
</form>

<% for (var i = 0; i < files.length; i++){ %>
	<a href='/photo/<%=files[i]%>'> <%= files[i]%>  </a>
	<br>
<% } %>